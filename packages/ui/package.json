{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./style.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "lint": "eslint . --fix --cache",
    "spell": "cspell \"**/*.{js,ts,mjs,cjs,json,css,less,scss,vue,html,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist node_modules",
    "clean:deps": "rimraf node_modules",
    "clean:artifacts": "rimraf dist"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/spell-config": "workspace:*",
    "@repo/test-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/unocss-config": "workspace:*",
    "@repo/vite-config": "workspace:*",
    "@types/node": "^24.10.9",
    "@unocss/eslint-config": "^66.6.0",
    "@vueuse/core": "^14.1.0",
    "element-plus": "^2.13.2",
    "eslint-plugin-oxlint": "~1.38.0",
    "sass-embedded": "^1.97.3",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.16",
    "vue": "^3.5.26",
    "vue-tsc": "^3.2.2"
  },
  "peerDependencies": {
    "element-plus": "^2.13.2",
    "unocss": "^66.6.0",
    "vue": "^3.5.26"
  }
}
