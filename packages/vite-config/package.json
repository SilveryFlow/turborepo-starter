{
  "name": "@repo/vite-config",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli \"**/*.{js,ts,mjs,cjs,json,css,less,scss,vue,html,md}\" --ignore-unknown --cache --cache-location .prettiercache",
    "spell": "cspell \"**/*.{js,ts,mjs,cjs,json,css,less,scss,vue,html,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist node_modules",
    "clean:deps": "rimraf node_modules",
    "clean:artifacts": "rimraf dist"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-icons": "^23.0.1",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-font": "^5.1.2",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vite-plugin-restart": "^2.0.0",
    "vite-plugin-vue-devtools": "^8.0.5"
  },
  "peerDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "unocss": "^66.6.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-icons": "^23.0.1",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-font": "^5.1.2",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vite-plugin-restart": "^2.0.0",
    "vite-plugin-vue-devtools": "^8.0.5"
  }
}
